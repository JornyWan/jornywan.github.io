source 'https://rubygems.org'

# Use the github-pages gem for GitHub Pages compatibility
gem 'github-pages', group: :jekyll_plugins

# Essential plugins that are supported by GitHub Pages
group :jekyll_plugins do
    gem 'jekyll-feed'
    gem 'jekyll-sitemap'
    gem 'jekyll-paginate'
    gem 'jekyll-seo-tag'
    gem 'jemoji'
end
